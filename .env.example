ANALYZER_BACKEND=gemini

# --- Gemini (ANALYZER_BACKEND=gemini のとき必須) ---
GEMINI_API_KEY=replace_me
GEMINI_MODEL=gemini-pro-vision
GEMINI_MAX_TOKENS=1024
GEMINI_TEMPERATURE=0.4

# 429（クオータ/レート制限）対策
GEMINI_MAX_RETRIES=5
GEMINI_RETRY_BUFFER_SECONDS=0.5
# 例: free tier 5 req/min の場合は 12 を推奨
GEMINI_REQUEST_SPACING_SECONDS=0

# --- Local LLM (ANALYZER_BACKEND=local) / LM Studio ---
# OpenAI互換API: http://localhost:1234/v1
LOCAL_LLM_BASE_URL=http://localhost:1234/v1
# LM Studio側のモデル名。未指定/auto/local-model の場合は /v1/models から自動選択
LOCAL_LLM_MODEL=auto
LOCAL_LLM_MAX_TOKENS=1024
LOCAL_LLM_TEMPERATURE=0.4
LOCAL_LLM_TIMEOUT_SECONDS=60
NANOBANANA_API_KEY=
NANOBANANA_ENDPOINT=https://api.nanobanana.pro/v1/images
NANOBANANA_MODEL=nano-pro-vision
ENABLE_VISUALIZATION=false
TIMEZONE=Asia/Tokyo
CAPTURE_INTERVAL_SECONDS=60
IDLE_THRESHOLD_MINUTES=5
CAPTURE_ROOT=data/captures
ARCHIVE_ROOT=data/archive
DELETE_CAPTURE_AFTER_ANALYSIS=true
SUMMARY_OUTPUT_DIR=output
REPORT_EXPORT_DIR=reports
LOG_DIR=logs
LOG_LEVEL=INFO